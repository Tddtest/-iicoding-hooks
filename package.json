{
  "name": "@iicoding/hooks",
  "version": "0.0.8",
  "main": "dist/index.mjs",
  "type": "module",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "rm -fr dist && father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build",
    "release": "npm run build && npm run updateVersion && npm publish --access public && npm run push",
    "updateVersion": "node ./update-version.js",
    "transfer": "node ./transfer.js",
    "push": "npm run transfer && cd ../iicoding-hooks && sh ./git.sh push"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tddtest/iicoding-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/Tddtest/iicoding-hooks/issues"
  },
  "homepage": "https://github.com/Tddtest/iicoding-hooks#readme",
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "peerDependencies": {
    "react": "^16.12.0 || ^17.0.0 || ^18.2.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/lodash-es": "^4.17.12",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "antd": "^4.20.7",
    "babel-plugin-import": "^1.13.5",
    "dumi": "^1.1.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "lodash-es": "^4.17.21",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  }
}